{
  "name": "fastfast/common",
  "description": "fastfast common service functions and classes",
  "type": "library",
  "require": {

    "php": "^8.1",
    "illuminate/support": "^10.10",
    "react/event-loop": "^1.5",
    "react/promise": "^3.2",
    "symfony/property-access": "^7.3",
    "aws/aws-sdk-php": "^3.349",
    "illuminate/console": "^10.48",
    "kreait/firebase-php": "^7.21",
    "pusher/pusher-php-server": "^7.2",
    "edamov/pushok": "^0.18.1"
  },
  "autoload": {
    "psr-4": {
      "FastFast\\Common\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "authors": [
    {
      "name": "Akeeb Ismail",
      "email": "ismailakeeb@gmail.com"
    }
  ],
  "extra": {
    "laravel": {
      "providers": [
        "FastFast\\Common\\FastFastCommonProvider"
      ]
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^10.5 || ^11.0 || ^12.0",
    "orchestra/testbench": "^8.21"
  }
}
